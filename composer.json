{
  "name": "dominik-eller/geoip",
  "description": "Google Ads GeoTargets CSV updater and lookup as a reusable PHP 8.4 Composer package.",
  "type": "library",
  "license": "MIT",
  "keywords": ["google ads", "geotargets", "geo", "location", "criteria_id", "lookup"],
  "require": {
    "php": "^8.4",
    "ext-json": "*",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Deller\\GeoIP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deller\\GeoIP\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/geotargets-update",
    "bin/geotargets-lookup"
  ],
  "scripts": {
    "test": "phpunit",
    "tests": "phpunit"
  },
  "support": {
    "issues": "https://example.com/dominik-eller/geoip/issues"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
